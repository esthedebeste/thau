{
	"compilerOptions": {
		"noEmit": true,
		"target": "ES2019",
		"module": "ESNext",
		"moduleResolution": "node",
		"isolatedModules": true,
		"downlevelIteration": true,
		"noImplicitAny": false,
		"noUnusedParameters": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true
	}
}
