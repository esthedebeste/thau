{
	"include": ["src"],
	"compilerOptions": {
		"target": "ES2019",
		"module": "ESNext",
		"outDir": "dist",
		"baseUrl": "src",
		"moduleResolution": "node",
		"isolatedModules": true,
		"downlevelIteration": true,
		"noImplicitAny": false,
		"noUnusedParameters": true,
		"forceConsistentCasingInFileNames": true,
		"declaration": true
	}
}
